{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/cs2-launcher.sln",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "runOptions": {
        "instanceLimit": 1
      },
      "problemMatcher": "$msCompile",
      "isBuildCommand": true,
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}